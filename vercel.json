{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/chat/(.*)",
      "dest": "/public/chat.html"
    },
    {
      "src": "/groups",
      "dest": "/public/groups.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}